{
  "version": 2,
  "name": "KrakenD api gateway to third-party",
  "port": 8080,
  "timeout": "10000ms",
  "extra_config": {
    "github_com/devopsfaith/krakend-cors": {
      "allow_origins": ["http://qastaging.siigo.com"],
      "allow_methods": ["POST", "GET", "PUT", "DELETE"],
      "allow_headers": ["*"]
    }
  },
  "endpoints": [
    {
      "endpoint": "api/third-party/account/{id}",
      "output_encoding": "no-op",
      "headers_to_pass": ["*"],
      "querystring_params": ["*"],
      "method": "GET",
      "backend": [
        {
          "method": "GET",
          "host": ["http://ms-thirdparty:5000"],
          "url_pattern": "api/third-party/account/{id}",
          "encoding": "no-op"
        }
      ]
    },
    {
      "endpoint": "api/third-party/account/{id}",
      "output_encoding": "no-op",
      "headers_to_pass": ["*"],
      "querystring_params": ["*"],
      "method": "DELETE",
