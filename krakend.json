        {
          "method": "DELETE",
          "host": ["http://ms-thirdparty:5000"],
          "url_pattern": "api/third-party/account/{id}",
          "encoding": "no-op"
        }
      ]
    },
    {
      "endpoint": "api/third-party/account",
      "output_encoding": "no-op",
      "headers_to_pass": ["*"],
      "querystring_params": ["*"],
